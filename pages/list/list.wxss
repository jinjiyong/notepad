.page{
  position: relative;
}
.list{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.item{
  display: flex;
  flex-direction: column;
  margin-bottom: 20rpx;
  width: 100%;
  height: 150rpx;
  background: #fff;
}
.content{
  border-bottom: 1px dashed #d0d0d0;
  padding: 0 20rpx;
  height: 80rpx;
  color: #333;
  line-height: 80rpx;
}
.time{
  padding: 0 20rpx;
  color: #999;
  font-size: 30rpx;
  line-height: 70rpx;
}
.add{
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  right: 50rpx;
  bottom: 50rpx;
  border: 1px solid #ff660d;
  border-radius: 50%;
  width: 80rpx;
  height: 80rpx;
}
.add image{
  width: 50rpx;
  height: 50rpx;
}